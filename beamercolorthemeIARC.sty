    \ProvidesPackage{beamercolorthemeIARC}[15/06/2017 v1.0 beamer 'IARC' color theme]

    \mode<presentation>

    \definecolor{green}{RGB}{0,155,118}
    \definecolor{orange}{RGB}{233,131,0}
    \definecolor{darkgray}{RGB}{77,79,83}
    \definecolor{lightgray}{RGB}{182,177,169}
    \definecolor{beige}{RGB}{210,194,149}
    \definecolor{darkbeige}{RGB}{179,153,93}

	\definecolor{IARCblue}{RGB}{0,153,204}

	\setbeamercolor{reverse}{bg=white,fg=black}
    \setbeamercolor{normal text}{fg=black,bg=white}
    \setbeamercolor{alerted text}{fg=orange}
    \setbeamercolor{example text}{fg=IARCblue}
    \setbeamercolor{structure}{fg=IARCblue}
    \setbeamercolor{section in head/foot}{bg=IARCblue,fg=white}
    \setbeamercolor{subsection in head/foot}{bg=white,fg=black}
   
   \setbeamercolor{title}{fg=IARCblue}
    \setbeamercolor{author}{fg=lightgray}
    \setbeamercolor{date}{fg=lightgray}
    \setbeamercolor{framesubtitle}{fg=lightgray}

    \mode<all>

